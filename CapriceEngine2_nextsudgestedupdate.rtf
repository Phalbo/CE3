\par }{\rtlch\fcs1 \af1 \ltrch\fcs0 \insrsid7893721 // CapricEngine \endash  Modal Interchange System Extension (v2.20 compatible)
\par 
\par // --------------------
\par // 1. MODAL INTERCHANGE DICTIONARY
\par // --------------------
\par const MODAL_INTERCHANGE = \{
\par   Ionian: ["Lydian", "Mixolydian", "Dorian", "Aeolian"],
\par   Dorian: ["Aeolian", "Phrygian", "Mixolydian"],
\par   Phrygian: ["Dorian", "Aeolian"],
\par   Lydian: ["Ionian", "Mixolydian"],
\par   Mixolydian: ["Ionian", "Dorian", "Aeolian"],
\par   Aeolian: ["Dorian", "Phrygian", "Mixolydian"],
\par   Locrian: ["Phrygian", "Aeolian"]
\par \};
\par 
\par // --------------------
\par // 2. UTILITY FUNCTION \endash  GET ALL CHORDS IN A MODE
\par // --------------------
\par function getChordsInMode(key, mode) \{
\par   const scaleNotes = getNotesInScale(key, mode);
\par   const chords = [];
\par   for (let i = 0; i < scaleNotes.length; i++) \{
\par     const chordType = getChordTypeForModeDegree(mode, i);
\par     chords.push(\{
\par       root: scaleNotes[i],
\par       type: chordType,
\par       degree: i + 1
\par     \});
\par   \}
\par   return chords;
\par \}
\par 
\par // --------------------
\par // 3. FUNCTION \endash  GET INTERCHANGE CHORDS
\par // --------------------
\par function getInterchangeChords(key, mode) \{
\par   const borrowedChords = [];
\par   const alternateModes = MODAL_INTERCHANGE[mode] || [];
\par   alternateModes.forEach(altMode => \{
\par     const chords = getChordsInMode(key, altMode);
\par     chords.forEach(ch => \{
\par       borrowedChords.push(\{
\par         root: ch.root,
\par         type: ch.type,
\par         degree: ch.degree,
\par         sourceMode: altMode
\par       \});
\par     \});
\par   \});
\par   return borrowedChords;
\par \}
\par 
